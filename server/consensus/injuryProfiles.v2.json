  "calculation_logic": {
    "multiplier_formula": "base_impact × age_factor × recurrence_adjustment",
    "age_factor_calculation": "decay_rate ^ max(0, age - threshold)",
    "recurrence_adjustment": "1.0 - (recurrence_risk × 0.1)",
    "partial_season_interpolation": "linear blend from year_of_injury to year_after_return based on weeksRecovered/avg_recovery_weeks"
